<form action="" bindsubmit="login">
    <label for="user">用户名:</label>
    <input type="text" id="user"  name="user" placeholder-class="txt" placeholder="请输入用户名"/>
    <text class="tips">{{user}}</text>
    <label for="pass">密 码:</label>
    <input type="password" id="pass" name="pass" placeholder-class="txt" placeholder="请输入密码"/>
    <text class="tips">{{pass}}</text>
    <button type="default" form-type="submit" style="width: 50%;">登录</button>
</form>
